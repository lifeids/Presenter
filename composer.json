{
    "name": "lifeids/eloquent-presenter",
    "description": "Model Presenters for Laravel",
    "keywords": ["laravel", "framework", "Eloquent-Presenter", "Eloquent Presenter"],
    "license": "MIT",
    "authors": [{
        "name": "Lifeids",
        "email": "info@lifeids.com",
        "homepage": "https://lifeids.com"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/database": "5.5.* || 5.6.*",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Lifeids\\Presenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lifeids\\Tests\\Presenter\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
